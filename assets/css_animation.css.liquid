///////////css_animation/////////
.fadeUp{
    animation-name: fade_up;
    animation-duration:1s;
    animation-fill-mode:forwards;
}
.fadeUpTrigger{
    opacity:1;
}

@keyframes fade_up {
    0% {
      opacity: 0;
      transform:translateY(200px);
    }
    100% {
      opacity: 1;
      transform:translateY(0%);
    }
}